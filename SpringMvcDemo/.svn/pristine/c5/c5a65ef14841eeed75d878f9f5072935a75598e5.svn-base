package com.zdcf.service.Impl;

import java.util.List;
import java.util.Map;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

import com.zdcf.mapper.ChatRecordsMapper;
import com.zdcf.service.ChatRecordService;

@Service
@Transactional
public class ChatRecordServiceImpl implements  ChatRecordService {

	@Autowired
	private ChatRecordsMapper chatRecordsMapper;
	
	@Override
	public List<Map<String, Object>> queryChatRecords(String user) {
		return chatRecordsMapper.queryChatRecords(user);
	}


}
